{
  "name": "tslint-no-commented-code-rule",
  "version": "0.1.0",
  "description": "TSLint rule that bans commented code",
  "main": "rule-config.json",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexkuz/tslint-no-commented-code-rule.git"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "tslint": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexkuz/tslint-no-commented-code-rule/issues"
  },
  "homepage": "https://github.com/alexkuz/tslint-no-commented-code-rule#readme"
}
